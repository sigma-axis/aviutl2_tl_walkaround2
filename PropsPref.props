<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros" />
  <PropertyGroup>
    <IntDir>$(SolutionDir)$(Configuration)\intermed\</IntDir>
    <GenerateManifest>false</GenerateManifest>
    <OutDir>$(SolutionDir)$(Configuration)\</OutDir>
    <_PropertySheetDisplayName>Props Preference</_PropertySheetDisplayName>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <TreatWarningAsError>true</TreatWarningAsError>
      <LanguageStandard>stdcpplatest</LanguageStandard>
      <UseFullPaths>false</UseFullPaths>
      <AdditionalOptions>/source-charset:utf-8 /execution-charset:utf-8 %(AdditionalOptions)</AdditionalOptions>
      <UseStandardPreprocessor>true</UseStandardPreprocessor>
      <WarningLevel>Level3</WarningLevel>
      <ConformanceMode>true</ConformanceMode>
    </ClCompile>
    <Link>
      <EnableUAC>false</EnableUAC>
    </Link>
  </ItemDefinitionGroup>
  <ItemGroup />
</Project>